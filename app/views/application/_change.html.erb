<% @apis.each do |api| %>
  <div class="small-12 columns api-listing text-left detail-text-medium float-center">
    <div class="row">
        <div style="padding: 10px">
          <%= link_to(api.name, api_path(api)) %>
          <%= api.url %>
          <%= api.description %>
        </div>
    </div>
  </div>
  <%= will_paginate @apis,
  page_links: false,
  renderer: FoundationPagination::Rails %>
<% end %>
